package projetPoec;

import java.util.Date;

public class Video {
	private int idVideo;
	private String title;
	private String author;
	private double duration;
	private int like;
	private int dislike;
	private Date publicationDate;
	private int vuesCounter;
	private User user;
	public Video(int idVideo, String title, String author, double duration, int like, int dislike, Date publicationDate,
			int vuesCounter) {
		super();
		this.idVideo = idVideo;
		this.title = title;
		this.author = author;
		this.duration = duration;
		this.like = like;
		this.dislike = dislike;
		this.publicationDate = publicationDate;
		this.vuesCounter = vuesCounter;
	}
	public int getIdVideo() {
		return idVideo;
	}
	public void setIdVideo(int idVideo) {
		this.idVideo = idVideo;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getAuthor() {
		return author;
	}
	public void setAuthor(String author) {
		this.author = author;
	}
	public double getDuration() {
		return duration;
	}
	public void setDuration(double duration) {
		this.duration = duration;
	}
	public int getLike() {
		return like;
	}
	public void setLike(int like) {
		this.like = like;
	}
	public int getDislike() {
		return dislike;
	}
	public void setDislike(int dislike) {
		this.dislike = dislike;
	}
	public Date getPublicationDate() {
		return publicationDate;
	}
	public void setPublicationDate(Date publicationDate) {
		this.publicationDate = publicationDate;
	}
	public int getVuesCounter() {
		return vuesCounter;
	}
	public void setVuesCounter(int vuesCounter) {
		this.vuesCounter = vuesCounter;
	}

}
